{
  "name": "sprachy",
  "version": "0.1.0",
  "private": true,
  "main": "server/dist/worker.js",
  "scripts": {
    "faunadev": "mkdirp devdb && docker run --rm --name faunadb -p 8443:8443 -p 8084:8084 -v /tmp/devdb:/var/lib/faunadb fauna/faunadb",
    "client": "webpack serve --config webpack.client.config.js",
    "server-webpack": "webpack -w --config webpack.server.config.js",
    "server-miniflare": "miniflare --kv=STORE --watch --debug --build-command=\"\" server/devdist/worker.js",
    "server": "concurrently --kill-others npm:server-webpack npm:server-miniflare",
    "dev": "concurrently --kill-others npm:faunadev npm:client npm:server-webpack npm:server-miniflare",
    "build-live-client": "rm -rf client/dist/* && API_BASE_URL=https://sprachy.suns.workers.dev webpack --mode production --config webpack.client.config.js",
    "build-staging-client": "rm -rf client/dist/* && API_BASE_URL=https://sprachy-staging.suns.workers.dev webpack --mode production --config webpack.client.config.js",
    "build-server": "rm -rf server/dist/* && webpack --mode production --config webpack.server.config.js",
    "build-live": "yarn build-live-client && yarn build-server",
    "td": "yarn build && wrangler publish --env test",
    "deploy": "yarn typecheck && yarn test && yarn build-live && yarn deploy-server && yarn deploy-client",
    "deploy-server": "wrangler publish --env live",
    "deploy-client": "netlify deploy -d client/dist --prod",
    "typecheck": "tsc -p server/tsconfig.json --noEmit && tsc -p client/tsconfig.json --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@sentry/browser": "^6.13.3",
    "@types/lodash": "^4.14.169",
    "@types/marked": "^3.0.1",
    "@types/uuid": "^8.3.1",
    "accessible-nprogress": "^2.1.2",
    "axios": "^0.22.0",
    "bootstrap": "4.6.0",
    "bootstrap-vue": "^2.21.2",
    "bootswatch": "^5.0.1",
    "cookie": "^0.4.1",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.6.5",
    "css-loader": "^6.2.0",
    "csv-loader": "^3.0.3",
    "d3-ease": "^3.0.1",
    "dotenv-webpack": "^7.0.3",
    "fauna-shell": "^0.12.6",
    "faunadb": "^4.4.1",
    "lodash": "^4.17.21",
    "marked": "^3.0.4",
    "mini-css-extract-plugin": "^2.4.2",
    "node-repl-await": "^0.1.2",
    "sade": "^1.7.4",
    "sass": "1.32.13",
    "sass-loader": "10.1.1",
    "shelljs": "^0.8.4",
    "slugify": "^1.6.0",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.3",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-loader": "^15.9.7",
    "vue-meta": "^2.4.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.2",
    "vue-runtime-template-compiler": "^1.3.0",
    "vue-template-compiler": "^2.6.14",
    "vue-timeago": "^5.1.3",
