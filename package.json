    "sass": "^1.49.10",
    "sass-loader": "12.6.0",
    "string-pixel-width": "^1.10.0",
    "svelte-fa": "^2.4.0",
    "svelte-markdown": "^0.2.2",
    "sveltestrap": "^5.9.0",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.2",
    "zod": "^3.14.3"
