{
  "name": "vokabon",
  "version": "0.1.0",
  "private": true,
  "main": "server/dist/worker.js",
  "scripts": {
    "client": "webpack serve --config webpack.client.config.js",
    "server-webpack": "webpack -w --config webpack.server.config.js",
    "server-miniflare": "miniflare --watch --debug --build-command=\"\" server/devdist/worker.js",
    "server": "npm-run-all --parallel server-webpack server-miniflare",
    "dev": "npm-run-all --parallel client server",
    "build-client": "rm -rf client/dist/* && webpack --mode production --config webpack.client.config.js",
    "build-server": "rm -rf server/dist/* && webpack --mode production --config webpack.server.config.js",
    "build": "npm-run-all build-client build-server",
    "td": "yarn build && wrangler publish --env test",
    "deploy": "yarn typecheck && yarn test && yarn build && wrangler publish --env live",
    "typecheck": "tsc -p server/tsconfig.json --noEmit && tsc -p client/tsconfig.json --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@fauna-labs/fauna-schema-migrate": "^2.1.5",
    "@supabase/supabase-js": "^1.24.0",
    "@types/lodash": "^4.14.169",
    "@types/marked": "^3.0.1",
    "accessible-nprogress": "^2.1.2",
    "axios": "^0.22.0",
    "bootstrap": "4.5.3",
    "bootstrap-vue": "^2.21.2",
    "bootswatch": "^5.0.1",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^6.2.0",
    "csv-loader": "^3.0.3",
    "dotenv-webpack": "^7.0.3",
    "faunadb": "^4.4.1",
    "kip-migrate": "file:../kip",
    "lodash": "^4.17.21",
    "marked": "^3.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "postgres": "^1.0.2",
    "restyped-axios": "^2.0.0",
    "sass": "^1.34.1",
    "sass-loader": "10.1.1",
    "shelljs": "^0.8.4",
    "slugify": "^1.6.0",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.3",
    "tslib": "^2.2.0",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-loader": "^15.9.7",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.38.1",
    "worktop": "^0.7.3",
    "zod": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@cloudflare/workers-types": "^2.2.2",
    "@types/child-process-promise": "^2.2.2",
    "@types/jest": "^27.0.1",
    "babel-jest": "^27.1.0",
    "child-process-promise": "^2.2.1",
    "dotenv": "^10.0.0",
    "ifdef-loader": "^2.3.0",
    "jest": "^27.1.0",
    "ley": "^0.7.1",
    "miniflare": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "prompt-confirm": "^2.0.4",
    "supabase": "^0.5.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
