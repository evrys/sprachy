{
  "name": "sprachy",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "env-cmd svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "npm run check && wrangler publish --env live && node ./scripts/post-deploy.js",
    "stealthdeploy": "wrangler publish --env live",
    "td": "wrangler publish --env staging",
    "resetdb": "NODE_OPTIONS='--experimental-specifier-resolution=node' env-cmd node --loader ts-node/esm scripts/resetdb.ts",
    "adminify": "NODE_OPTIONS='--experimental-specifier-resolution=node' env-cmd node --loader ts-node/esm scripts/adminify.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@cloudflare/wrangler": "^1.19.11",
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.36",
    "@sveltejs/kit": "^1.0.0-next.304",
    "@types/bootstrap": "^5.1.9",
    "@types/cookie": "^0.4.1",
    "@types/node": "^17.0.23",
    "@types/shelljs": "^0.8.11",
    "@types/string-pixel-width": "^1.7.2",
    "@types/tough-cookie": "^4.0.1",
    "axios-cookiejar-support": "^2.0.4",
    "dotenv": "^16.0.0",
    "miniflare": "^2.3.0",
    "shelljs": "^0.8.5",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.1",
    "tough-cookie": "^4.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite-plugin-iso-import": "^0.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@sentry/browser": "^6.19.3",
    "@types/htmlparser2": "^3.10.3",
    "@types/lodash": "^4.14.181",
    "@types/uuid": "^8.3.4",
    "accessible-nprogress": "^2.1.2",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "cookie": "^0.4.2",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.21.1",
    "env-cmd": "^10.1.0",
    "fauna-shell": "^0.14.0",
