{
  "name": "sprachy",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "env-cmd --no-override svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "npm run check && npm run build && wrangler publish --env live && node ./scripts/post-deploy.js",
    "stealthdeploy": "npm run build && wrangler publish --env live",
    "td": "npm run build && wrangler publish --env staging",
    "initdb": "NODE_OPTIONS='--experimental-specifier-resolution=node' env-cmd --no-override node --loader ts-node/esm scripts/initdb.ts",
    "adminify": "NODE_OPTIONS='--experimental-specifier-resolution=node' env-cmd --no-override node --loader ts-node/esm scripts/adminify.ts",
    "test": "env-cmd --no-override vitest",
    "faunadb": "docker run -d --rm --name fauna -p 8443:8443 -p 8084:8084 -v devdb:/var/lib/fauna fauna/faunadb"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@sveltejs/kit": "^1.0.0-next.391",
    "@types/bootstrap": "^5.2.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.0.6",
    "@types/shelljs": "^0.8.11",
    "@types/spark-md5": "^3.0.2",
    "@types/string-pixel-width": "^1.7.2",
    "@types/tough-cookie": "^4.0.2",
    "axios-cookiejar-support": "^4.0.2",
    "dotenv": "^16.0.1",
    "miniflare": "^2.6.0",
    "shelljs": "^0.8.5",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tough-cookie": "^4.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite-plugin-iso-import": "^0.1.3",
    "vitest": "^0.18.1",
    "vitest-svelte-kit": "^0.0.6",
    "wrangler": "^2.0.22"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@sentry/browser": "^7.7.0",
    "@stripe/stripe-js": "^1.32.0",
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.51",
    "@types/htmlparser2": "^3.10.3",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "accessible-nprogress": "^2.1.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "cookie": "^0.5.0",
    "core-js": "^3.23.5",
    "env-cmd": "^10.1.0",
    "faunadb": "^4.6.0",
    "htmlparser2": "^8.0.1",
    "lodash": "^4.17.21",
    "object-to-formdata": "^4.4.2",
    "path-to-regexp": "^6.2.1",
    "slug": "^5.3.0",
    "spark-md5": "^3.0.2",
    "string-pixel-width": "^1.10.0",
    "stripe": "^9.14.0",
    "svelte-fa": "^3.0.3",
    "svelte-markdown": "^0.2.2",
    "sveltestrap": "github:laxadev/sveltestrap",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.2",
    "web-auth-library": "^0.4.1",
    "zod": "^3.17.10"
  }
}
