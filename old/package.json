{
  "name": "sprachy",
  "version": "0.1.0",
  "private": true,
  "main": "server/dist/worker.js",
  "scripts": {
    "stopfauna": "(docker rm faunadb || true) | dev-null",
    "faunadev": "mkdirp ./devdb && npm run --silent stopfauna && docker run --name faunadb -p 8443:8443 -p 8084:8084 -v $(pwd)/devdb:/var/lib/faunadb fauna/faunadb",
    "client": "webpack serve --config webpack.client.config.js",
    "server-webpack": "webpack -w --config webpack.server.config.js",
    "server-miniflare": "mkdirp server/devdist && touch server/devdist/worker.js && npx miniflare --kv=STORE --watch --debug server/devdist/worker.js",
    "server": "concurrently --kill-others npm:server-webpack npm:server-miniflare",
    "dev": "concurrently --kill-others npm:faunadev npm:client npm:server-webpack npm:server-miniflare",
    "build-live-client": "rm -rf client/dist/* && API_BASE_URL=https://sprachy.suns.workers.dev webpack --mode production --config webpack.client.config.js",
    "build-staging-client": "rm -rf client/dist/* && API_BASE_URL=https://sprachy-staging.suns.workers.dev webpack --mode production --config webpack.client.config.js",
    "build-server": "rm -rf server/dist/* && webpack --mode production --config webpack.server.config.js",
    "build-live": "npm run build-live-client && npm run build-server",
    "td": "npm run build && wrangler publish --env test",
    "deploy": "npm run check && npm run test && npm run build-live && npm run deploy-server && npm run deploy-client",
    "really-deploy": "npm run build-live && npm run deploy-server && npm run deploy-client",
    "deploy-server": "wrangler publish --env live",
    "deploy-client": "netlify deploy -d client/dist --prod",
    "check": "tsc -p server/tsconfig.json --noEmit && tsc -p client/tsconfig.json --noEmit && svelte-check",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@sentry/browser": "^6.18.1",
    "@types/lodash": "^4.14.179",
    "@types/uuid": "^8.3.4",
    "accessible-nprogress": "^2.1.2",
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "cookie": "^0.4.2",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.21.1",
    "css-loader": "^6.7.0",
    "csv-loader": "^3.0.3",
    "d3-ease": "^3.0.1",
    "dotenv-webpack": "^7.1.0",
    "fauna-shell": "^0.14.0",
    "faunadb": "^4.5.2",
    "fontawesome-svelte": "^2.0.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "miniflare": "^2.3.0",
    "node-repl-await": "^0.1.2",
    "object-to-formdata": "^4.4.2",
    "sade": "^1.8.1",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "shelljs": "^0.8.5",
    "slugify": "^1.6.5",
    "sv-bootstrap-dropdown": "^0.4.4",
    "svelte": "^3.46.4",
    "svelte-loader": "^3.1.2",
    "svelte-markdown": "^0.2.2",
    "svelte-navigator": "^3.1.5",
    "svelte-preprocess": "4.10.4",
    "timeago-simple": "^1.2.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "worktop": "^0.7.3",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@cloudflare/workers-types": "^3.4.0",
    "@cloudflare/wrangler": "^1.19.8",
    "@types/bootstrap": "^5.1.9",
    "@types/child-process-promise": "^2.2.2",
    "@types/cookie": "^0.4.1",
    "@types/d3-ease": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/sade": "^1.7.4",
    "@types/shelljs": "^0.8.11",
    "@types/tough-cookie": "^4.0.1",
    "axios-cookiejar-support": "^2.0.4",
    "babel-jest": "^27.5.1",
    "child-process-promise": "^2.2.1",
    "concurrently": "^7.0.0",
    "dev-null-cli": "^2.0.0",
    "dotenv": "^16.0.0",
    "ifdef-loader": "^2.3.2",
    "jest": "^27.5.1",
    "jest-environment-miniflare": "^2.3.0",
    "mkdirp": "^1.0.4",
    "prompt-confirm": "^2.0.4",
    "svelte-check": "^2.4.5",
    "tough-cookie": "^4.0.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
